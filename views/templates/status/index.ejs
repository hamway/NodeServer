<div class="page-header">
    <h1>
        <%= title %> <small>Statistics</small>
    </h1>
</div>
<p class="text-left">
	Server is <strong>developed</strong> now, <em>version dev-0.1.0</em>.
</p>
<div class="row-fluid" onclick="$(this).find('pre').toggle();">
<div>Status info ^</div>
<pre style="display: none">
<%= statusRaw %>
</pre>
</div>

<div class="row-fluid" style="margin-top: 15px;">
	<style type="text/css">
		@import "/stylesheets/chart.css";
	</style>

	<div class="row-fluid">
		<div class="span12">
	        <div class="chart">
		        <div id="chartMemory" data-percent="0">
			        <div class="percent">0%</div>
			        </div>
				<div class="label:empty">Memory</div>
	        </div>
		</div>
	</div>

	<script type="text/javascript" src="/components/jquery-chart/dist/jquery.easypiechart.min.js"></script>
	<script type="text/javascript" src="/javascripts/status.js"></script>

	<script type="text/javascript">
		$(function() {
			Status.init('<%= status %>');
		});
	</script>
</div>
